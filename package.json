{
  "name": "@namchee/tetikus",
  "version": "0.3.0",
  "description": "A custom cursor component for Vue 3 ✌️",
  "author": "Cristopher Namchee <cristophernamchee12@gmail.com>",
  "scripts": {
    "lint": "eslint",
    "build": "rm -rf dist/ && rollup -c",
    "build:watch": "npm run build -- --watch",
    "build:verbose": "npm run build -- --verbose",
    "test": "ava",
    "postuninstall": "npm prune"
  },
  "main": "dist/tetikus.cjs.js",
  "module": "dist/tetikus.esm.js",
  "unpkg": "dist/tetikus.min.js",
  "cdn": "dist/tetikus.min.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/compiler-sfc": "^3.0.0-rc.10",
    "@vue/eslint-config-typescript": "^5.0.2",
    "ava": "^3.12.1",
    "eslint": "^6.7.2",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-vue": "^7.0.0-alpha.0",
    "esm": "^3.2.25",
    "postcss-nesting": "^7.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.10",
    "rollup-plugin-scss": "^2.6.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sinon": "^9.0.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~3.9.3",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "ava": {
    "files": [
      "src/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "esm",
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "verbose": true
  },
  "files": [
    "src",
    "dist"
  ],
  "homepage": "https://github.com/Namchee/tetikus",
  "keywords": [
    "javascript",
    "vuejs",
    "vuejs3",
    "custom-cursor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Namchee/tetikus.git"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/Namchee/tetikus/issues"
  },
  "license": "MIT"
}
