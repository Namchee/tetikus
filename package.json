{
  "name": "@namchee/tetikus",
  "version": "0.1.0",
  "description": "A custom cursor component for Vue 3 ✌️",
  "author": "Cristopher Namchee <cristophernamchee12@gmail.com>",
  "scripts": {
    "build": "vue-cli-service build --target lib --name tetikus ./src/index.ts",
    "lint": "vue-cli-service lint",
    "build:report": "npm run build -- --report",
    "build:watch": "npm run build -- --watch",
    "build:bili": "rm -rf dist/ && bili src/index.ts",
    "postuninstall": "npm prune",
    "test": "ava"
  },
  "main": "dist/tetikus.common.js",
  "unpkg": "dist/tetikus.umd.min.js",
  "cdn": "dist/tetikus.umd.min.js",
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "^4.5.4",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-plugin-typescript": "^4.5.4",
    "@vue/cli-service": "^4.5.4",
    "@vue/compiler-sfc": "^3.0.0-rc.10",
    "@vue/eslint-config-typescript": "^5.0.2",
    "ava": "^3.12.1",
    "bili": "^5.0.5",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-vue": "^7.0.0-alpha.0",
    "esm": "^3.2.25",
    "rimraf": "^3.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sinon": "^9.0.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~3.9.3",
    "vue": "^3.0.0-rc.10"
  },
  "ava": {
    "files": [
      "src/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "esm",
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "verbose": true
  },
  "files": [
    "src/*",
    "dist/*"
  ],
  "homepage": "https://github.com/Namchee/tetikus",
  "keywords": [
    "javascript",
    "vuejs",
    "vuejs3",
    "custom-cursor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Namchee/tetikus.git"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/Namchee/tetikus/issues"
  },
  "license": "MIT"
}
